{
  "build": {
    "devPath": "../src",
    "distDir": "../src",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "miqs_music_prot",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "path": {
        "all": true
      }
    },
    "windows": [
      {
        "title": "miqs_music_prot",
        "width": 960,
        "height": 720
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.miqsmusicprot.dev",
      "icon": [
        "./icons/32x32.png",
        "./icons/128x128.png",
        "./icons/128x128@2x.png",
        "./icons/icon.icns",
        "./icons/icon.ico"
      ]
    }
  }
}
